            <field name="typical_students">28</field>
            <field name="student_satisfaction">4.7</field>
            <field name="course_rating">4.6</field>
            <field name="meta_title">Machine Learning - IF305</field>
            <field name="meta_description">Mata kuliah pilihan semester 5. Belajar teknik machine learning dan aplikasi dalam data science.</field>
            <field name="meta_keywords">machine learning, data science, IF305, Python, AI</field>
            <field name="website_published">True</field>
