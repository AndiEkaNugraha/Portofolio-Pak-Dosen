        <record id="mata_kuliah_post_financial_economics" model="mata_kuliah.post">
            <field name="name">Financial Economics</field>
            <field name="subtitle">Teori ekonomi keuangan dan aplikasi dalam pasar modal</field>
            <field name="blog_id" ref="mata_kuliah_blog_graduate"/>
            <field name="slug">financial-economics</field>
            <field name="course_code">ECO501</field>
            <field name="semester">1</field>
